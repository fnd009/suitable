{
    "name": "laravolt/suitable",
    "description": "Semantic-UI table builder for Laravel application",
    "keywords": [
        "laravel",
        "laravolt",
        "table",
        "crud",
        "suitable",
        "semantic UI"
    ],
    "homepage": "https://github.com/laravolt/suitable",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "illuminate/support": "5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "suggest": {
        "lavary/laravel-menu": "Required to render Dropdown column"
    },
    "require-dev": {
        "phpunit/phpunit" : "~5.0",
        "orchestra/testbench": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Suitable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Suitable\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Suitable\\ServiceProvider"
            ],
            "aliases": {
                "Suitable": "Laravolt\\Suitable\\Facade"
            }
        }
    }
}
